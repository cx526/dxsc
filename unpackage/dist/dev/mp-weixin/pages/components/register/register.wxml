<view><view class="register"><view class="logo"><image src="/static/images/user-logo.png" mode></image></view><view class="choose"><view data-event-opts="{{[['tap',[['login']]]]}}" bindtap="__e">登录</view><view>注册</view><view></view></view><view class="form-list"><view class="form-item"><image src="/static/images/arrow-user.png" mode></image><input type="text" value="{{username}}" placeholder="请输入用户名" placeholder-class="placeholder" data-event-opts="{{[['blur',[['user']]],['input',[['__set_model',['','username','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/></view><view class="form-item"><image src="/static/images/arrow-phone.png" mode></image><input type="text" value="{{cellphone}}" placeholder="请输入手机号" placeholder-class="placeholder" data-event-opts="{{[['blur',[['phone']]],['input',[['__set_model',['','cellphone','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/></view><view class="form-item"><image src="/static/images/arrow-password.png" mode></image><input type="password" value="{{code}}" placeholder="请输入密码" placeholder-class="placeholder" data-event-opts="{{[['blur',[['password']]],['input',[['__set_model',['','code','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/></view><view class="form-item"><input type="text" value="{{vertification}}" placeholder="请输入手机验证码" placeholder-class="placeholder" data-event-opts="{{[['blur',[['QRcode']]],['input',[['__set_model',['','vertification','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/><button class="QRcode" size="mini" disabled="{{disabled}}" data-event-opts="{{[['tap',[['getCode']]]]}}" bindtap="__e"><block wx:if="{{get}}"><text>获取验证码</text></block><block wx:if="{{down}}"><text>{{"还剩"+count+"秒"}}</text></block><block wx:if="{{again}}"><text>重新获取</text></block></button></view></view><block wx:if="{{tabbar}}"><view class="btn"><view data-event-opts="{{[['tap',[['register']]]]}}" class="login" bindtap="__e"><button>马上注册</button></view><view class="register"><button>微信注册</button></view></view></block></view></view>