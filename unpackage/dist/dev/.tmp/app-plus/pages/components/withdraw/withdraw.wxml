<view><view class="money-box"><view class="money"><view style="margin:0 30rpx;padding:24rpx 0;"><text>金额</text></view><view class="import" style="flex:1;"><view><input type="number" value="{{price}}" placeholder="请输入提现的金额" data-event-opts="{{[['input',[['__set_model',['','price','$event',[]]]]]]}}" bindinput="__e"/></view><view><text style="font-size:28rpx;color:#CECECE;">{{"可提取余额￥"+(userBalance>0?userBalance:0)}}</text></view></view></view></view><view class="way"><text>输入提现方式</text></view><view class="choose-box"><view class="choose"><view class="choose-item"><view><label data-event-opts="{{[['tap',[['bank',['$event']]]]]}}" class="radio" bindtap="__e"><radio checked="{{tixian_type==0?true:false}}" color="#FED940"></radio><text></text></label><text style="margin-left:20rpx;">银行卡提现</text></view><block wx:if="{{account_number_list.length>0}}"><view><picker mode="selector" value="{{bank_id}}" range-key="name_num" range="{{account_number_list}}" data-event-opts="{{[['change',[['chooseBank',['$event']]]]]}}" bindchange="__e"><view>{{account_number_list[bank_id].name_num}}</view></picker></view></block><block wx:else><block wx:if="{{tixian_type==0}}"><view data-event-opts="{{[['tap',[['goBankCard',['$event']]]]]}}" bindtap="__e">未绑定银行卡,点击添加!</view></block></block><view><image src="{{$src+'/images/icon-right.png'}}"></image></view></view></view></view><view class="btn"><button data-event-opts="{{[['tap',[['tixian',['$event']]]]]}}" bindtap="__e">提交</button></view></view>