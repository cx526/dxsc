.recommend-box {
  box-sizing: border-box;
  background: #F5F5F5;
  padding: 0 26rpx;
}
.recommend-box .recommend {
    box-sizing: border-box;
}
.recommend-box .recommend .recommend-item {
      padding: 28rpx 20rpx;
      box-sizing: border-box;
      background: #fff;
      border-radius: 8rpx;
      overflow: hidden;
      margin-top: 20rpx;
}
.recommend-box .recommend .recommend-item .recommend-left {
        box-sizing: border-box;
        float: left;
        width: 165rpx;
        height: 165rpx;
}
.recommend-box .recommend .recommend-item .recommend-left image {
          width: 100%;
          height: 100%;
}
.recommend-box .recommend .recommend-item .recommend-right {
        box-sizing: border-box;
        padding-left: 16rpx;
        float: left;
        width: calc(100% - 165rpx);
}
.recommend-box .recommend .recommend-item .recommend-right .title {
          box-sizing: border-box;
          width: 100%;
          line-height: 30rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .title .main {
            font-size: 30rpx;
            font-weight: 700;
            color: #000;
            display: inline-block;
            max-width: 86%;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
}
.recommend-box .recommend .recommend-item .recommend-right .title view {
            display: inline-block;
            font-size: 22rpx;
            margin-left: 18rpx;
            position: relative;
            padding: 4rpx 6rpx;
            background: #FFF5ED;
            color: #F19C49;
}
.recommend-box .recommend .recommend-item .recommend-right .address {
          font-size: 28rpx;
          margin-top: 16rpx;
          color: #ccc;
}
.recommend-box .recommend .recommend-item .recommend-right .rate {
          box-sizing: border-box;
          display: -webkit-box;
          display: -webkit-flex;
          display: flex;
          -webkit-box-align: center;
          -webkit-align-items: center;
                  align-items: center;
          -webkit-box-pack: justify;
          -webkit-justify-content: space-between;
                  justify-content: space-between;
          margin-top: 6rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .rate .rate-left {
            -webkit-box-flex: 1;
            -webkit-flex: 1;
                    flex: 1;
            box-sizing: border-box;
            color: #656565;
            font-size: 22rpx;
            display: -webkit-box;
            display: -webkit-flex;
            display: flex;
}
.recommend-box .recommend .recommend-item .recommend-right .rate .rate-left .score {
              box-sizing: border-box;
}
.recommend-box .recommend .recommend-item .recommend-right .rate .rate-left .score image {
                width: 25rpx;
                height: 25rpx;
                margin-right: 6rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .rate .rate-left .goal {
              margin-left: 12rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .rate .rate-left .symbol {
              margin-left: 12rpx;
              font-size: 16rpx;
              position: relative;
              top: 5rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .rate .rate-right {
            box-sizing: border-box;
            font-size: 22rpx;
            color: #656565;
}
.recommend-box .recommend .recommend-item .recommend-right .feature {
          box-sizing: border-box;
          display: -webkit-box;
          display: -webkit-flex;
          display: flex;
          -webkit-box-pack: justify;
          -webkit-justify-content: space-between;
                  justify-content: space-between;
          -webkit-box-align: center;
          -webkit-align-items: center;
                  align-items: center;
          font-size: 22rpx;
          color: #656565;
          margin-top: 6rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .feature .feature-left {
            box-sizing: border-box;
            -webkit-box-flex: 1;
            -webkit-flex: 1;
                    flex: 1;
            max-width: 86%;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
}
.recommend-box .recommend .recommend-item .recommend-right .feature .feature-left text {
              margin-top: 12rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .feature .feature-right {
            box-sizing: border-box;
            color: #FF4407;
}
.recommend-box .recommend .recommend-item .recommend-right .ranking {
          box-sizing: border-box;
          font-size: 22rpx;
          color: #FF4407;
          margin-top: 10rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .ranking text {
            border: 1px solid #EAEAEA;
            padding: 3rpx 6rpx;
            display: inline-block;
            border-radius: 20rpx;
            border-bottom-left-radius: 0;
            margin-right: 8rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .ranking text:last-child {
            margin-right: 0;
}
.recommend-box .recommend .recommend-item .recommend-right .ranking text.active {
            border-color: #FFD6C8;
}
.recommend-box .recommend .recommend-item .recommend-right .group {
          font-size: 20rpx;
          color: #5F5F5F;
          margin-top: 16rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .group text:first-child {
            color: #fff;
            display: inline-block;
            padding: 2rpx 6rpx;
            margin-right: 10rpx;
            background: #FD490A;
            border-radius: 2rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .ticket {
          font-size: 20rpx;
          color: #5F5F5F;
          margin-top: 16rpx;
}
.recommend-box .recommend .recommend-item .recommend-right .ticket text:first-child {
            color: #fff;
            display: inline-block;
            padding: 2rpx 6rpx;
            margin-right: 10rpx;
            background: #F99802;
            border-radius: 2rpx;
}

