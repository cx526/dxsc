page {
  background: #EFEFF4;
}
.order-number {
  box-sizing: border-box;
  padding: 0 30rpx;
  background: #fff;
}
.order-number .number {
    line-height: 90rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
            justify-content: space-between;
    color: #444444;
}
.order-number .number text:nth-child(2) {
      color: #AAAAAA;
}
.logistic-box {
  box-sizing: border-box;
  padding: 30rpx;
  margin-top: 20rpx;
  background: #fff;
}
.logistic-box .logistic {
    box-sizing: border-box;
    font-size: 30rpx;
    color: #888888;
}
.logistic-box .logistic .type {
      box-sizing: border-box;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.logistic-box .logistic .type image {
        width: 30rpx;
        height: 30rpx;
        position: relative;
        top: 6rpx;
}
.logistic-box .logistic .user-info {
      box-sizing: border-box;
      margin-top: 20rpx;
}
.logistic-box .logistic .user-info .user {
        box-sizing: border-box;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: justify;
        -webkit-justify-content: space-between;
                justify-content: space-between;
}
.logistic-box .logistic .user-info .address {
        box-sizing: border-box;
        margin-top: 18rpx;
        display: -webkit-box;
        display: -webkit-flex;
        display: flex;
        -webkit-box-orient: vertical;
        -webkit-box-direction: normal;
        -webkit-flex-direction: column;
                flex-direction: column;
}
.goods-box {
  box-sizing: border-box;
  margin-top: 20rpx;
  background: #fff;
  border-top: 1px solid #E5E5E5;
}
.goods-box .goods {
    box-sizing: border-box;
}
.goods-box .goods .goods-title {
      box-sizing: border-box;
      padding: 0 30rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
      font-size: 24rpx;
      color: #AAAAAA;
      border-bottom: 1px solid #E5E5E5;
      line-height: 70rpx;
}
.goods-box .goods .goods-title text:nth-child(1) {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
                flex: 1;
}
.goods-box .goods .product {
      box-sizing: border-box;
      padding: 30rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      border-bottom: 1px solid #E5E5E5;
}
.goods-box .goods .product .product-img {
        width: 140rpx;
        height: 140rpx;
        -webkit-flex-shrink: 0;
                flex-shrink: 0;
}
.goods-box .goods .product .product-img image {
          width: 100%;
          height: 100%;
}
.goods-box .goods .product .product-title {
        box-sizing: border-box;
        padding-left: 30rpx;
        padding-right: 48rpx;
}
.goods-box .goods .product .product-title .title {
          font-size: 30rpx;
          color: #444444;
          display: -webkit-box;
          -webkit-line-clamp: 3;
          -webkit-box-orient: vertical;
          overflow: hidden;
}
.goods-box .goods .product .product-title .params {
          font-size: 24rpx;
          color: #AAAAAA;
}
.goods-box .goods .product .product-price {
        box-sizing: border-box;
}
.goods-box .goods .product .product-price .price {
          font-size: 30rpx;
          color: #000;
}
.goods-box .goods .product .product-price .number {
          font-size: 24rpx;
          color: #AAAAAA;
          text-align: right;
}
.goods-box .goods .total {
      box-sizing: border-box;
      padding: 0 30rpx;
      font-size: 24rpx;
      border-bottom: 1px solid #E5E5E5;
      padding-bottom: 18rpx;
      padding-top: 20rpx;
      text-align: right;
}
.goods-box .goods .total view:nth-child(2) {
        box-sizing: border-box;
        margin-top: 20rpx;
        font-size: 24rpx;
}
.goods-box .goods .total view:nth-child(2) text {
          display: inline-block;
          box-sizing: border-box;
          border: 1px solid #f00;
          width: 120rpx;
          height: 44rpx;
          line-height: 44rpx;
          text-align: center;
}
.goods-box .goods .total view:nth-child(2) text:nth-child(1) {
          margin-right: 14rpx;
          border: 1px solid #DDDDDD;
          color: #9B9B9B;
}
.goods-box .goods .total view:nth-child(2) text:nth-child(2) {
          border: 1px solid #B4302D;
          color: #B4302D;
}
.order-time {
  box-sizing: border-box;
  margin-top: 20rpx;
  padding: 30rpx;
  background: #fff;
  color: #888888;
  font-size: 30rpx;
}
.order-time .time {
    box-sizing: border-box;
}
.order-time .time view {
      box-sizing: border-box;
      margin-top: 6rpx;
}
.order-time .time view:nth-child(1) {
      margin-top: 0;
}

