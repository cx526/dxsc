<view><view class="nav-box"><view class="nav"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['handleNav',[index]]]]]}}" class="{{['nav-item ',(currentIndex==index)?'active':'']}}" bindtap="__e"><text>{{item}}</text></view></block></block></view></view><block wx:if="{{visible==1}}"><view class="animated fadeInLeft"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="goods-box"><view class="goods"><view class="goods-title"><text>{{item.create_date}}</text><text>{{item.status_name}}</text></view><block wx:for="{{item.order_item_list}}" wx:for-item="good" wx:for-index="i" wx:key="i"><block><view data-event-opts="{{[['tap',[['orderDetail',['$0'],[[['list','',index,'order_id']]]]]]]}}" class="product" bindtap="__e"><view class="product-img"><image src="{{baseURL+good.picture.pic_cover}}" mode></image></view><view class="product-title"><view class="title"><text>{{good.goods_name}}</text></view><view class="params"><text>{{good.sku_name}}</text></view></view><view class="product-price"><view class="price"><text>{{"￥"+good.price}}</text></view><view class="number"><text>{{"×"+good.num}}</text></view></view></view></block></block><view class="total"><view><text>{{"共"+item.order_item_list.length+"件商品，合计：￥"+item.order_money+"(含运费￥"+item.shipping_money+")"}}</text></view><view><block wx:for="{{item.member_operation}}" wx:for-item="btn" wx:for-index="bk" wx:key="bk"><block><block wx:if="{{btn.no=='pay'}}"><text>{{btn.name}}</text></block><block wx:if="{{btn.no=='close'}}"><text data-event-opts="{{[['tap',[['orderClose',['$0'],[[['list','',index,'order_id']]]]]]]}}" bindtap="__e">{{btn.name}}</text></block><block wx:if="{{btn.no=='logistics'}}"><text data-event-opts="{{[['tap',[['orderLogistics',['$0'],[[['list','',index,'order_id']]]]]]]}}" bindtap="__e">{{btn.name}}</text></block><block wx:if="{{btn.no=='getdelivery'}}"><text data-event-opts="{{[['tap',[['OrderTakeDelivery',['$0'],[[['list','',index,'order_id']]]]]]]}}" bindtap="__e">{{btn.name}}</text></block><block wx:if="{{btn.no=='delete_order'}}"><text data-event-opts="{{[['tap',[['orderDeleteOrder',['$0'],[[['list','',index,'order_id']]]]]]]}}" bindtap="__e">{{btn.name}}</text></block></block></block></view></view></view></view></block></block></view></block></view>