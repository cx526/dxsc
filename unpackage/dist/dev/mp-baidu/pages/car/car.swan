<block s-if="{{loading}}"><view><view class="list-box"><view class="list-title"><view style="flex:1;"></view><text data-event-opts="{{[['tap',[['getLocartion']]]]}}" bindtap="__e">{{locationName}}</text><image src="{{$src+'/images/location.png'}}"></image></view><block s-if="{{list.length==0}}"><view class="list-empty">您所在的位置暂无爆品哦</view></block><block s-if="{{list}}"><view class="list"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><block><view data-event-opts="{{[['tap',[['downLineDetail',['$0'],[[['list','',index,'goods_id']]]]]]]}}" class="list-item" bindtap="__e"><view class="list-img"><image src="{{baseURL+item.picture_cover}}" mode="aspectFill"></image></view><view class="list-context"><view class="list-address"><text class="name">{{item.shop_name}}</text><view class="address"><text class="hide">{{''+item.live_store_address+''}}</text><text class="line"></text><text>{{item.distance_number}}</text></view></view><view class="list-main"><text style="position:relative;top:2rpx;">{{item.goods_name}}</text></view><view class="list-price"><view class="now-price"><view class="price"><text>{{item.price}}</text><text style="font-size:26rpx;font-weight:normal;">元</text></view><button type="warn">购买</button></view><view class="old-price"><text style="text-decoration:line-through;">{{'原价：'+item.market_price+''}}</text><text>{{"销售量："+item.sales}}</text></view></view></view></view></block></block><block s-if="{{!loadMore&&list.length!=0}}"><view class="none">已加载全部</view></block></view></block></view></view></block><block s-else><loading vue-id="29688bb2-1"></loading></block>