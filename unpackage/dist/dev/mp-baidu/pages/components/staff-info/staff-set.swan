<view><view class="set-box"><view class="set-line"><view class="set-label">普通返点(%)</view><view class="set-input"><input class="input" data-key="0" type="digit" data-event-opts="{{[['input',[['changeInfo',['$event']]]]]}}" value="{{info[0]}}" bindinput="__e"/></view></view><view class="set-line"><view class="set-label">VIP返点(%)</view><view class="set-input"><input class="input" data-key="1" type="digit" data-event-opts="{{[['input',[['changeInfo',['$event']]]]]}}" value="{{info[1]}}" bindinput="__e"/></view></view><view class="set-line"><view class="set-label">商家返点(%)</view><view class="set-input"><input class="input" data-key="2" type="digit" data-event-opts="{{[['input',[['changeInfo',['$event']]]]]}}" value="{{info[2]}}" bindinput="__e"/></view></view><view class="set-line"><view class="set-label">团队长返点(%)</view><view class="set-input"><input class="input" data-key="3" type="digit" data-event-opts="{{[['input',[['changeInfo',['$event']]]]]}}" value="{{info[3]}}" bindinput="__e"/></view></view><view class="set-line"><view class="set-label">代理返点(%)</view><view class="set-input"><input class="input" data-key="4" type="digit" data-event-opts="{{[['input',[['changeInfo',['$event']]]]]}}" value="{{info[4]}}" bindinput="__e"/></view></view><view class="set-line"><view class="set-label">商家流水(%)</view><view class="set-input"><input class="input" data-key="5" type="digit" data-event-opts="{{[['input',[['changeInfo',['$event']]]]]}}" value="{{info[5]}}" bindinput="__e"/></view></view></view><view class="btn"><button data-event-opts="{{[['tap',[['saveSet',['$event']]]]]}}" style="background:#FEE16A;" bindtap="__e">保存</button></view></view>