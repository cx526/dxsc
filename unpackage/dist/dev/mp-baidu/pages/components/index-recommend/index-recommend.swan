<view class="recommend-box"><block s-if="{{recommendList.length>0}}"><view class="recommend"><block s-for="{{recommendList}}" s-for-item="item" s-for-index="index" s-key="index"><block><view data-event-opts="{{[['tap',[['goShop',['$0'],[[['recommendList','',index,'shop_id']]]]]]]}}" class="recommend-item" bindtap="__e"><view class="recommend-left"><block s-if="{{item.shop_logo==''}}"><image class="image" src="{{$src+'/images/none-logo.jpg'}}"></image></block><block s-if="{{item.shop_logo!=''}}"><easy-loadimage class="image" vue-id="{{'35748e84-1-'+index}}" scroll-top="{{scrollTop}}" image-src="{{baseURL+item.shop_logo}}"></easy-loadimage></block></view><view class="recommend-right"><view class="title"><text class="main">{{item.shop_name}}</text></view><view class="rate" style="margin-top:16rpx;"><view class="rate-left"><view class="score"><view><block s-if="{{item.shop_deliverycredit}}"><block><block s-for="{{item.all}}" s-for-item="all" s-for-index="allIndex" s-key="allIndex"><image src="{{$src+'/images/start-active.png'}}"></image></block><block s-for="{{item.half}}" s-for-item="half" s-for-index="halfIndex" s-key="halfIndex"><image src="{{$src+'/images/start-half.png'}}"></image></block><block s-for="{{item.none}}" s-for-item="none" s-for-index="noneIndex" s-key="noneIndex"><image src="{{$src+'/images/start.png'}}"></image></block></block></block><block s-else><text style="font-size:26rpx;">暂无评分</text></block><block s-if="{{item.shop_deliverycredit&&item.shop_deliverycredit!=0}}"><text class="goal">{{''+item.shop_deliverycredit+'分'}}</text></block></view><view class="rate-right"><text style="font-size:24rpx;color:#797979;">{{item.distance_number}}</text></view></view></view></view><view style="display:flex;align-items:center;justify-content:space-between;"><view style="font-size:26rpx;color:#797979;margin-right:16rpx;"><text>经营范围</text></view><image style="width:60rpx;height:26rpx;" src="{{$src+'/images/renqi.png'}}" mode></image></view><view class="address" style="margin-right:20rpx;"><block s-if="{{item.live_store_address}}"><text>{{item.live_store_address}}</text></block><block s-else><text>暂无该店铺相关地址信息</text></block></view></view></view></block></block></view></block><block s-else><view style="box-sizing:border-box;padding:30rpx;font-size:26rpx;text-align:center;">附件暂无商铺</view></block></view>