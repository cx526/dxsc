<view class="container"><view class="classify-search"><view class="search"><view class="search-input"><image src="{{$src+'/images/search.png'}}"></image><input type="text" placeholder="输入您要搜索的宝贝.." placeholder-class="placeholder" data-event-opts="{{[['input',[['handleSearch',['$event']]]]]}}" bindinput="__e"/></view><view class="search-img"><text data-event-opts="{{[['tap',[['search',['$event']]]]]}}" bindtap="__e">搜索</text></view></view></view><view class="list-box"><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['handleClick',['$0'],[[['list','',index]]]]]]]}}" class="list-item" bindtap="__e"><text>{{item}}</text><image src="/static/images/icon-right.png"></image></view></block></view></view>