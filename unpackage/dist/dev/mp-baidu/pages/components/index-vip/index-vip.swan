<view class="vip-box"><view class="vip-goods"><scroll-view scroll-x="true"><view class="goods-list"><block s-for="{{vip_product}}" s-for-item="item" s-for-index="index" s-key="index"><block><view data-event-opts="{{[['tap',[['goDetail',[index]]]]]}}" class="goods" bindtap="__e"><image src="{{item.pic_cover_small}}"></image><view class="goods-title"><text>{{item.goods_name}}</text></view><view class="goods-price"><view><text class="new-price">{{"优惠价："+item.price}}</text></view><view class="old-price"><text>{{"市场价："+item.market_price}}</text></view></view></view></block></block></view></scroll-view></view><view class="vip-get" style="margin:50rpx 0;"><image src="{{$src+'/images/member.png'}}" data-event-opts="{{[['tap',[['goMoney',['$event']]]]]}}" bindtap="__e"></image></view></view>