<view><view class="product-main"><view class="product-online"><scroll-view style="{{'height:'+(statusHeight)+';'}}" scroll-y="true" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]]]}}" bindscrolltolower="__e"><block s-if="{{dataList&&dataList.length>0}}"><block><block s-for="{{dataList}}" s-for-item="item" s-for-index="index" s-key="index"><view data-event-opts="{{[['tap',[['handleDetail',['$0'],[[['dataList','',index,'goods_id']]]]]]]}}" class="online-item" bindtap="__e"><view class="online-show"><image src="{{item.pic_cover_small}}"></image></view><view class="online-des"><view class="online-title"><text>{{item.goods_name}}</text></view><view class="online-context"><view class="online-price"><view style="display:flex;flex-direction:column;"><text style="color:#FC3D3C;">{{'优惠价：'+item.price+''}}</text><text style="color:#AAAAAA;font-size:22rpx;text-decoration:line-through;">{{'市场价：'+item.market_price+''}}</text></view></view><view class="online-current"><text>{{"库存："+item.stock}}</text></view></view></view></view></block></block></block><block s-if="{{dataList==''}}"><view class="none">暂无相关折扣商品</view></block><block s-if="{{!flag}}"><view class="none">暂无更多数据</view></block></scroll-view></view></view></view>