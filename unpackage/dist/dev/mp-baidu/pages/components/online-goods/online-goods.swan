<view><view class="nav"><scroll-view class="scroll" scroll-x="true" scroll-into-view="{{id}}"><block s-for="{{typeList}}" s-for-item="item" s-for-index="index" s-key="category_id"><block><view class="{{[currentIndex==index?'active':'']}}" id="{{item.id}}" data-event-opts="{{[['tap',[['handleClick',['$0',index],[[['typeList','category_id',item.category_id]]]]]]]}}" bindtap="__e">{{item.short_name}}</view></block></block></scroll-view><view data-event-opts="{{[['tap',[['goClassify',['$event']]]]]}}" class="menu" bindtap="__e"><image src="{{$src+'/images/icon-menu.png'}}" mode></image></view></view><view class="banner"><image src="{{$src+'/images/online-goods.png'}}" mode></image></view><view class="recommend"><view class="title"><text style="position:relative;z-index:999;">为你推荐</text><view class="line"></view></view><block s-if="{{onLineGoods&&onLineGoods.length>0}}"><view class="list"><block s-for="{{onLineGoods}}" s-for-item="goods" s-for-index="index" s-key="index"><block><view data-event-opts="{{[['tap',[['handleDetail',['$0'],[[['onLineGoods','',index,'goods_id']]]]]]]}}" class="list-item" bindtap="__e"><easy-loadimage class="image" vue-id="{{'68fc23c8-1-'+index}}" scroll-top="{{scrollTop}}" image-src="{{goods.pic_cover_mid}}"></easy-loadimage><view class="main"><view class="list-title">{{goods.goods_name}}</view><view class="list-price"><view class="new"><text>{{"￥"+goods.price}}</text><text class="member">会员专享价</text></view><view class="old"><text>市场参考价：</text><text>{{"￥"+goods.market_price}}</text></view></view></view></view></block></block></view></block><block s-else><view class="none">暂无数据</view></block><block s-if="{{flag}}"><view class="none">暂无数据</view></block></view></view>