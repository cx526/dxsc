<view><view class="check-title"><view class="check"><view class="radio"><checkbox style="transform:scale(0.7);" color="#f00" checked="{{selectAllchecked}}" data-event-opts="{{[['tap',[['sellectAll']]]]}}" bindtap="__e"></checkbox><text>全选</text></view><view class="edit"><text data-event-opts="{{[['tap',[['edit']]]]}}" style="font-size:26rpx;" bindtap="__e">{{isShow?'编辑':'完成'}}</text></view></view></view><view class="goods-box"><view class="goods"><block s-for="{{goods_list}}" s-for-item="goods" s-for-index="index" s-key="index"><block><view class="product"><view class="product-checkbox"><label><checkbox style="transform:scale(0.7);" color="#f00" checked="{{goods.act}}" data-event-opts="{{[['tap',[['checkBox',[index]]]]]}}" bindtap="__e"></checkbox></label></view><view class="product-img"><image src="{{baseUrl+goods.picture_info.pic_cover}}" mode></image></view><view class="product-title"><view class="title"><text>{{goods.goods_name}}</text></view><view class="params"><text>{{goods.sku_name}}</text></view></view><view class="product-price"><view class="price"><text>{{"￥"+goods.price}}</text></view><view class="number"><text>{{"×"+goods.num}}</text></view><view class="amount"></view></view><block s-if="{{!isShow}}"><view class="product-amount"><text data-event-opts="{{[['tap',[['decrement',[index]]]]]}}" bindtap="__e">-</text><input type="number" disabled value="{{goods.num}}"/><text data-event-opts="{{[['tap',[['increment',[index]]]]]}}" bindtap="__e">+</text></view></block></view></block></block></view></view><view class="btn"><view class="total" style="background:#333;"><text style="color:#fff;">合计：</text><text>{{"￥"+sumGoodsPrice}}</text></view><view data-event-opts="{{[['tap',[['submitOrder']]]]}}" class="add" style="background:#FED940;" bindtap="__e"><text>结算</text></view></view></view>