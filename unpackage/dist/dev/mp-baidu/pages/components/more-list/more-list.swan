<view><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" indicator-active-color="#fff" indicator-color="rgba(255,255,255,0.5)"><block s-for="{{3}}" s-for-item="n" s-for-index="__i0__" s-key="*this"><block><swiper-item><view class="swiper-item"><image src="{{$src+'/images/dianxia.png'}}" mode></image></view></swiper-item></block></block></swiper><view class="title"><text>为您推荐</text></view><view class="nav-box"><view class="nav"><block s-for="{{navList.list}}" s-for-item="item" s-for-index="index" s-key="index"><block><text data-event-opts="{{[['tap',[['changeNav',[index]]]]]}}" class="{{[navList.currentIndex==index?'active':'']}}" bindtap="__e">{{''+item.name+''}}</text></block></block></view></view><view class="product-box"><view class="product"><block s-for="{{goodsList}}" s-for-item="item" s-for-index="index" s-key="goods_id"><block><view data-event-opts="{{[['tap',[['goodsDetail',['$0'],[[['goodsList','goods_id',item.goods_id,'goods_id']]]]]]]}}" class="product-item" bindtap="__e"><image src="{{item.pic_cover_small}}" mode></image><view class="product-title"><text>{{item.goods_name}}</text></view><view class="product-price"><text>{{"￥"+item.price}}</text><text>{{"￥"+item.market_price}}</text></view></view></block></block></view><block s-if="{{!flag}}"><view class="none"><text>暂无更多数据</text></view></block></view></view>