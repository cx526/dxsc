<view><view class="tab-box"><block s-for="{{packet}}" s-for-item="v" s-for-index="i" s-key="i"><block><block s-if="{{curPacket==i}}"><view class="item focus">{{v.packet_name}}</view></block><block s-if="{{curPacket!=i}}"><view data-event-opts="{{[['tap',[['changePacket',[i]]]]]}}" class="item" bindtap="__e">{{v.packet_name}}</view></block></block></block></view><view class="goods-box"><block s-for="{{packet[curPacket].order_goods_list}}" s-for-item="v" s-for-index="i" s-key="i"><block><view class="item"><image src="{{baseURL+v.picture_info.pic_cover}}"></image></view></block></block></view><view class="cut-line"></view><view class="title-box"><view class="title"><view class="title-context"><view class="title-express"><text>{{packet[curPacket].express_name}}</text></view><view class="title-service"><text>{{"运单编号："+packet[curPacket].express_code}}</text></view></view></view></view><view class="cut-line"></view><view class="express-after"><text>物流跟踪</text></view><view class="express-status"><view class="status"><view class="express-item"><block s-for="{{express}}" s-for-item="item" s-for-index="index" s-key="index"><block><view class="item"><view style="margin-left:30rpx;"><view class="message"><text class="circle"></text><text>{{item.AcceptStation}}</text></view><view class="time"><text>{{item.AcceptTime}}</text></view></view></view></block></block></view></view></view></view>