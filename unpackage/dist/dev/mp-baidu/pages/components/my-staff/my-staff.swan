<view><view class="search-box"><view class="search"><image src="{{$src+'/images/search.png'}}" mode></image><input type="text" placeholder="搜索我的员工" data-event-opts="{{[['confirm',[['search',['$event']]]]]}}" bindconfirm="__e"/></view></view><view class="group-box"><view class="group"><view class="group-item" style="padding-left:20rpx;"><text>员工合计</text><text style="margin-left:26rpx;">{{count+"人"}}</text></view><view class="group-item" style="padding-right:20rpx;text-align:right;"><text>今日新增</text><text style="margin-right:26rpx;">{{newadd+"人"}}</text></view></view></view><view class="group-list"><view class="group-title"><text class="number">序号</text><text class="name">姓名</text><text class="message">信息</text></view><block s-for="{{list}}" s-for-item="item" s-for-index="index" s-key="index"><block><view data-event-opts="{{[['tap',[['goStaffInfo',['$0'],[[['list','',index,'uid']]]]]]]}}" class="group-title spcial" bindtap="__e"><text class="number hidden">{{index+1}}</text><text class="name hidden">{{item.nick_name}}</text><text class="message hidden">{{item.user_tel}}</text><image src="{{$src+'/images/icon-right.png'}}" mode></image></view></block></block></view></view>