<view><view class="register" style="{{'height:'+(clientHeight+'px')+';'}}"><view class="logo"><image src="{{$src+'/images/user-logo.png'}}" mode></image></view><view class="choose"><view></view><view>登录</view><view data-event-opts="{{[['tap',[['register']]]]}}" bindtap="__e">注册</view></view><view class="form-list"><view class="form-item"><image src="{{$src+'/images/arrow-user.png'}}" mode></image><input type="number" value="{{username}}" placeholder="请输入手机号" placeholder-class="input-placeholder" data-event-opts="{{[['blur',[['user',['$event']]]],['input',[['__set_model',['','username','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/></view><view class="form-item"><image src="{{$src+'/images/arrow-password.png'}}" mode></image><input type="password" value="{{password}}" placeholder="请输入密码" placeholder-class="input-placeholder" data-event-opts="{{[['blur',[['phone',['$event']]]],['input',[['__set_model',['','password','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/><text data-event-opts="{{[['tap',[['forget',['$event']]]]]}}" style="font-size:24rpx;position:absolute;right:10%;color:#C8C7CC;z-index:99;" bindtap="__e">忘记密码</text></view><view style="text-align:right;box-sizing:border-box;padding:0 10%;font-size:24rpx;color:#C8C7CC;"><text data-event-opts="{{[['tap',[['QRlogin',['$event']]]]]}}" bindtap="__e">手机验证码登录</text></view></view><block s-if="{{tabbar}}"><view class="btn"><view class="login"><button data-event-opts="{{[['tap',[['login']]]]}}" style="background:#E64340;color:#fff;" bindtap="__e">马上登录</button></view><view class="register"><button style="background:#04BE02;">微信登录</button></view></view></block></view></view>