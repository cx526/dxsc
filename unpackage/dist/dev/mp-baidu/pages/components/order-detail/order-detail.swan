<view><view class="order-number"><view class="number"><text>{{"订单号："+order.order_no}}</text><text>{{order.status_name}}</text></view></view><block s-if="{{order.shipping_type==2}}"><view class="logistic-box"><view class="logistic"><view class="type"><text>{{order.shipping_type_name}}</text></view><view class="user-info"><view class="user"><text>自提点</text></view><view class="address"><text>{{order['order_pickup']['province_name']+" "+order['order_pickup']['city_name']+" "+order['order_pickup']['dictrict_name']+" "+order['order_pickup']['address']}}</text></view></view></view></view></block><block s-if="{{order.shipping_type==1}}"><view class="logistic-box"><view class="logistic"><view class="type"><text>{{order.shipping_type_name}}</text></view><view class="user-info"><view class="user"><text>收货地址</text></view><view class="address"><view style="display:flex;justify-content:space-between;"><text>{{order['receiver_name']}}</text><text>{{order['receiver_mobile']}}</text></view><view style="margin-top:12rpx;"><text decode="true">{{order['address']}}</text><text>{{order['receiver_address']}}</text></view></view></view></view></view></block><block s-if="{{express_count>0}}"><view class="logistic-box"><view class="logistic"><view class="type"><text>{{express_name+"："+express_code}}</text><view data-event-opts="{{[['tap',[['orderLogistics',['$0'],['order.order_id']]]]]}}" bindtap="__e"><text>查物流</text><image src="{{$src+'/images/icon-right.png'}}" mode></image></view></view><view class="user-info"><view class="user"><text>{{order.receiver_name}}</text><text>{{order.receiver_mobile}}</text></view><view class="address"><text decode="true">{{order.address}}</text></view></view></view></view></block><view class="goods-box"><view class="goods"><view class="goods-title"><text>2020-3-20</text><text>{{order.shipping_type_name}}</text></view><block s-for="{{order.order_goods}}" s-for-item="good" s-for-index="i" s-key="i"><block><view class="product"><view class="product-img"><image src="{{baseURL+good.picture_info.pic_cover}}" mode></image></view><view class="product-title"><view class="title"><text>{{good.goods_name}}</text></view><view class="params"><text>{{good.sku_name}}</text></view></view><view class="product-price"><view class="price"><text>{{"￥"+good.price}}</text></view><view class="number"><text>{{"×"+good.num}}</text></view></view></view></block></block><view class="total"><view><text>{{"共"+order.order_goods.length+"件商品，合计：￥"+order.order_money+"(含运费￥"+order.shipping_money+")"}}</text></view><view><block s-for="{{order.operation}}" s-for-item="btn" s-for-index="bk" s-key="bk"><block><block s-if="{{btn.no=='pay'}}"><text data-event-opts="{{[['tap',[['orderPay',['$0'],['order.out_trade_no']]]]]}}" bindtap="__e">{{btn.name}}</text></block><block s-if="{{btn.no=='close'}}"><text>{{btn.name}}</text></block><block s-if="{{btn.no=='logistics'}}"><text data-event-opts="{{[['tap',[['orderLogistics',['$0'],['order.order_id']]]]]}}" bindtap="__e">{{btn.name}}</text></block><block s-if="{{btn.no=='getdelivery'}}"><text data-event-opts="{{[['tap',[['OrderTakeDelivery',['$0'],['order.order_id']]]]]}}" bindtap="__e">{{btn.name}}</text></block><block s-if="{{btn.no=='pickup'}}"><text>{{btn.name}}</text></block></block></block></view></view></view></view><view class="order-time"><view class="time"><block s-if="{{order.create_time>0}}"><view><text>{{"下单时间："+order.format_create_time}}</text></view></block><block s-if="{{order.pay_time>0}}"><view><text>{{"付款时间："+order.format_pay_time}}</text></view></block><block s-if="{{order.consign_time>0}}"><view><text>{{"发货时间："+order.format_consign_time}}</text></view></block></view></view></view>