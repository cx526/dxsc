<view><view class="classify-search"><view class="search"><view class="search-input"><image src="{{$src+'/images/search.png'}}"></image><input type="text" placeholder="输入您要搜索的宝贝.." placeholder-class="placeholder" data-event-opts="{{[['input',[['handleSearch',['$event']]]]]}}" bindinput="__e"/></view><view class="search-img"><text data-event-opts="{{[['tap',[['search',['$event']]]]]}}" bindtap="__e">搜索</text></view></view></view><view class="search-nav"><block s-for="{{navList}}" s-for-item="item" s-for-index="index" s-key="index"><block><view><text>{{item}}</text></view></block></block></view><view class="news-box"><view class="news"><block s-if="{{newsList&&newsList.length>0}}"><view><block s-for="{{newsList}}" s-for-item="item" s-for-index="index" s-key="index"><block><view data-event-opts="{{[['tap',[['handleGoodsDetail',['$0'],[[['newsList','',index,'goods_id']]]]]]]}}" class="news-item" bindtap="__e"><image src="{{item.pic_cover_small}}"></image><view class="news-text"><view class="news-title"><text>{{item.goods_name}}</text></view><view class="news-price"><text class="now">{{"￥"+item.market_price}}</text></view></view></view></block></block></view></block><block s-else><view style="text-align:center;font-size:28rpx;color:#444;padding:30rpx;">暂无对应的商品</view></block></view></view></view>