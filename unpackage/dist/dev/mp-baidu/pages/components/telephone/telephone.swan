<view><view class="register"><view class="logo"><image src="{{$src+'/images/user-logo.png'}}" mode></image></view><view class="form-list"><view class="form-item"><input type="text" value="{{cellphone}}" placeholder="请输入手机号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['','cellphone','$event',[]]]]]]}}" bindinput="__e"/></view><view class="form-item" style="margin-top:24rpx;"><input type="text" value="{{vertification}}" placeholder="请输入手机验证码" placeholder-class="placeholder" data-event-opts="{{[['blur',[['QRcode']]],['input',[['__set_model',['','vertification','$event',[]]]]]]}}" bindblur="__e" bindinput="__e"/><button class="QRcode" size="mini" disabled="{{disabled}}" data-event-opts="{{[['tap',[['getCode']]]]}}" bindtap="__e"><block s-if="{{get}}"><text>获取验证码</text></block><block s-if="{{down}}"><text>{{"还剩"+count+"秒"}}</text></block><block s-if="{{again}}"><text>重新获取</text></block></button></view></view><view class="btn"><view class="register"><button data-event-opts="{{[['tap',[['reset',['$event']]]]]}}" bindtap="__e">下一步</button></view></view></view></view>